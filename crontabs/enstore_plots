##
40 17 * * * enstore source /usr/local/etc/setups.sh; setup enstore; $ENSTORE_DIR/ecron -p file_family_usage "sh -c 'cd /home/enstore; . /usr/local/etc/setups.sh 1>>/dev/null 2>&1; setup postgres; python $ENSTORE_DIR/sbin/file_family_analysis.py'"  > /dev/null 2> /dev/null
