#!/bin/sh
set -u  # force better programming and ability to use check for not set
if [ "${1:-}" = "-x" ] ; then set -xv; shift; fi

# test/$RCSfile$  $Revision$

if [ -n "${1-}" ]; then
  opt="$1"
else
  opt=""
fi

emass $opt DM05-CA2901-8MM    < /dev/null &
emass $opt DM06-CA2902-8MM    < /dev/null &
emass $opt DM07-CA2903-8MM    < /dev/null &
emass $opt DM08-CA2904-8MM    < /dev/null &
emass $opt DC12-CA2251-8MM    < /dev/null &
emass $opt DE13-CA2501-DECDLT < /dev/null &
emass $opt DE14-CA2502-DECDLT < /dev/null &
emass $opt DE15-CA2503-DECDLT < /dev/null &
emass $opt DE16-CA2504-DECDLT < /dev/null &
