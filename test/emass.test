#!/bin/sh
set -u  # force better programming and ability to use check for not set
if [ "${1:-}" = "-x" ] ; then set -xv; shift; fi

# test/$RCSfile$  $Revision$

if [ -n "${1-}" ]; then
  opt="$1"
else
  opt=""
fi

emass $opt DM05-CA2901 < /dev/null > dm05.mount 2>&1 &
emass $opt DM06-CA2902 < /dev/null > dm06.mount 2>&1 &
emass $opt DM07-CA2903 < /dev/null > dm07.mount 2>&1 &
emass $opt DM08-CA2904 < /dev/null > dm08.mount 2>&1 &
emass $opt DC12-CA2251 < /dev/null > dc12.mount 2>&1 &
emass $opt DE13-CA2501 < /dev/null > de13.mount 2>&1 &
emass $opt DE14-CA2502 < /dev/null > de14.mount 2>&1 &
emass $opt DE15-CA2503 < /dev/null > de15.mount 2>&1 &
emass $opt DE16-CA2504 < /dev/null > de16.mount 2>&1 &
