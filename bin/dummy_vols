#!/bin/sh

if [ "${1:-}" = "-x" ] ; then set -xv; shift; fi

set -u  # force better programming and ability to use check for not set

. $ENSTORE_DIR/bin/defaults

x=0
while [ $x -le 1000 ] ; do
  x=`expr $x + 1`
  echo ecmd addvol shelf none redwood red$x   50000000000 50000000000
       ecmd addvol shelf none redwood red$x   50000000000 50000000000
done

