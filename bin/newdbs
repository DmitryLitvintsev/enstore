#!/bin/sh
set -x

stopall

# Blow away old databases
rm -f *.jou

serv_up

# Declare some new voluumes
python $ENSTORE_DIR/src/envoltool.py addvol activelibrary  family_raw floppy flop1 1400000 1400000
python $ENSTORE_DIR/src/envoltool.py addvol activelibrary  family_raw floppy flop2 1400000 1400000
python $ENSTORE_DIR/src/envoltool.py addvol activelibrary  family_process floppy flop22 1400000 1400000
python $ENSTORE_DIR/src/envoltool.py addvol shelf family_reserved floppy flop3 1400000 1400000
python $ENSTORE_DIR/src/envoltool.py addvol shelf family_reserved floppy flop4 1400000 1400000

loadfiles









