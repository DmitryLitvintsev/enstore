#!/bin/sh

#set -x

rm -f $ENSTORE_DIR/testfiles.in

dpnfs=/pnfs/enstore/test1-`whoami`
if [ -d $dpnfs ] ; then 
    rm $dpnfs/*
else
    mkdir $dpnfs
fi

if [ ! -d data ] ; then mkdir data ; fi
cd data
for f in * ; do 
    f2=$pnfs/`basename $f`
    python encp.py $f $f2
    echo $f2 >> ../files.in
done 

