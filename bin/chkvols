#!/bin/sh
set -u  # force better programming and ability to use check for not set
if [ "${1:-}" = "-x" ] ; then set -xv; shift; fi

# bin/$RCSfile$  $Revision$

vols=`ecmd vols | sed -e "s/'//g" -e "s/,/ /g"`
for vol in $vols; do
  #ecmd vcc --atmover $vol
  status=`ecmd vol $vol | egrep 'at_mover|label|inhibit'`
  echo $status
done
