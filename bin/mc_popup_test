#!/usr/bin/env wish 
button .b1 -text $argv -command {exit "0"}
button .b2 -text "Failure: not in home position in tower" -command {exit "9"}
button .b3 -text "Failure: drive already has volume mounted in it" -command {exit "8"}
button .b4 -text "Failure: code is None - very bad - CHECK ROBOT" -command {exit "7"}
button .b5 -text "Failure: check of volume failed" -command {exit "6"}
button .b6 -text "Failure: volume not found" -command {exit "5"}
button .b7 -text "Failure: check of drive failed" -command {exit "4"}
button .b8 -text "Failure: check of drive failed" -command {exit "3"}
pack .b1 .b2 .b3 .b4 .b5 .b6 .b7 .b8
