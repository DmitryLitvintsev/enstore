#!/bin/sh

start-config
if [ $? -ne 0 ] ; then exit 1; fi

python $ENSTORE_DIR/src/volume_clerk.py    --config_host localhost --config_port 7500 &
python $ENSTORE_DIR/src/file_clerk.py      --config_host localhost --config_port 7500 &
python $ENSTORE_DIR/src/library_manager.py --config_host localhost --config_port 7500 activelibrary &
python $ENSTORE_DIR/src/mover.py fd0.mover --config_host localhost --config_port 7500 &
sleep 1

