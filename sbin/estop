# sun nodes need an extended path in order to find all the regular unix commands
if [ `uname` = SunOS ]; then 
  PATH=$PATH:/bin:/sbin:/usr/bin:/usr/sbin:/usr/lbin:/usr/ucb:/usr/ccs/bin
fi

source /usr/local/etc/setups.sh
setup enstore
ecmd stop
/usr/local/bin/zapit mover
if   [ `uname` = Linux ]; then ps auxww |  egrep 'python|ecmd|encp|reader|writer|dasadmin|mt |db_';
elif [ `uname` = SunOS ]; then ps -ef   |  egrep 'python|ecmd|encp|reader|writer|dasadmin|mt |db_';
fi
