#!/bin/sh

if [ "${1:-}" = "-x" ] ; then set -xv; shift; fi

last6hours=`date --date "6 hours ago" +"%Y-%m-%d:%H"`
now=`date +"%Y-%m-%d:%H:%M:%S"`

python /home/enstore/enstore/src/plotter.py --sg --output_dir=/local/ups/prd/www_pages/enstore --start_time $last6hours

cp /local/ups/prd/www_pages/enstore/enplot_sg.ps /diska/enstore-log/enplot_sg/enplot_sg-$now.ps
