#!/bin/sh
set -u  # force better programming and ability to use check for not set

if [ "${1:-}" = "-x" ] ; then set -xv; shift; fi

# $Id$

node=`hostname -s`

case $node in

  stken*|fndca*|cmspnfs1|eagpnfs1|rip9|rip10)
	gang=stken
	;;
  rip*|cachen*)
	gang=rip
	;;
  d0en*)
	gang=d0en
	;;
  cdfen*)
	gang=cdfen
	;;
  *)
	gang=UNKNOWN
	;;
esac

echo $gang
