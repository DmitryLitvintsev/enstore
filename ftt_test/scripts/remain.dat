#==============================================================================
# This will verify the data from the test tape.
# It assumes that the tape has been written with twrite.
#==============================================================================
 
ftt_open 

ftt_rewind

ftt_echo "tape is rewound, remaining lines are remaining tape counters"
ftt_get_stats
ftt_extract_stat FTT_REMAIN_TAPE
ftt_write_tblock -n 64 -b 65536
ftt_close_dev

ftt_get_stats
ftt_extract_stat FTT_REMAIN_TAPE
ftt_write_tblock -n 64 -b 65536
ftt_close_dev

ftt_get_stats
ftt_extract_stat FTT_REMAIN_TAPE
ftt_write_tblock -n 64 -b 65536
ftt_close_dev

ftt_get_stats
ftt_extract_stat FTT_REMAIN_TAPE
ftt_write_tblock -n 64 -b 65536
ftt_close_dev

ftt_get_stats
ftt_extract_stat FTT_REMAIN_TAPE
ftt_write_tblock -n 64 -b 65536
ftt_close_dev

ftt_get_stats
ftt_extract_stat FTT_REMAIN_TAPE
ftt_write_tblock -n 64 -b 65536
ftt_close_dev

ftt_get_stats
ftt_extract_stat FTT_REMAIN_TAPE

ftt_rewind

ftt_close
quit

