<?xml version='1.0'?>
<!DOCTYPE NGOPConfig SYSTEM "system.dtd">
<NGOPConfig>
<Default_File/>
<StatusRulesSet ID="DefaultSystemRules">
  <DependList>
   <Group Name="{self}"/>
  </DependList>
  <DependRule Status="Good" Prio="0" SevLevel="None">
    <apply>
	<eq/>
	<apply>
	<min>
	 <bvar>i</bvar>
	  <lowlimit>
		<cn>0</cn>
	  </lowlimit>
	  <uperlimit><ci>%GroupLen({self})</ci></uperlimit>
          <apply>
                  <eq/>
                  <ci>{self}.State</ci>
                  <cn>1</cn>
          </apply>
	</min>
	  </apply>     
        <cn>1</cn>
         </apply>
  </DependRule>
  <DependRule Status="Error" Prio="2" SevLevel="0" Dsc="At_least_one_ME_is_in_bad_state:%ID" >
    <apply>
        <eq/>
         <apply>
        <max>
         <bvar>i</bvar>
          <lowlimit>
                <cn>0</cn>
          </lowlimit>
          <uperlimit><ci>%GroupLen({self})</ci></uperlimit>
          <apply>
                  <eq/>
                  <ci>{self}.Status</ci>
                  <cn>0</cn>
          </apply>
        </max>
          </apply>
        <cn>1</cn>
      </apply>

  </DependRule>
  <DependRule Status="Bad" Prio="2" SevLevel="0" Dsc="At_least_one_ME_is_in_bad_state:%ID" >
    <apply>
	<eq/>
	 <apply>
	<max>
	 <bvar>i</bvar>
	  <lowlimit>
		<cn>0</cn>
	  </lowlimit>
	  <uperlimit><ci>%GroupLen({self})</ci></uperlimit>
          <apply>
                  <eq/>
                  <ci>{self}.State</ci>
                  <cn>0</cn>
          </apply>
	</max>     
	  </apply>
        <cn>1</cn>
      </apply>

  </DependRule>


  <DependRule Status="Good" Prio="0" SevLevel="None"  Dsc="At_least_one_ME_is_undefined:%ID" >
    <apply>
	<eq/>
	 <apply>
	<max>
	 <bvar>i</bvar>
	  <lowlimit>
		<cn>0</cn>
	  </lowlimit>
	  <uperlimit><ci>%GroupLen({self})</ci></uperlimit>
          <apply>
               <apply>
                  <eq/>
                  <ci>{self}.State</ci>
                  <cn>-2</cn>
               </apply>
              </apply>
	</max> 
      </apply>
        <cn>1</cn>
         </apply>

  </DependRule>
  <DependRule Status="Unknown" Prio="1" SevLevel="None"  Dsc="All_MEs_are_unknown" >
    <apply>
	<eq/>
 	<apply>
	<min>
	 <bvar>i</bvar>
	  <lowlimit>
		<cn>0</cn>
	  </lowlimit>
	  <uperlimit><ci>%GroupLen({self})</ci></uperlimit>
          <apply>
               <apply>
                  <eq/>
                  <ci>{self}.State</ci>
                  <cn>-1</cn>
               </apply>
              </apply>
	</min> 
	      </apply>
        <cn>1</cn>
         </apply>

</DependRule>
  <DependRule Status="Undefined" Prio="1" SevLevel="None"  Dsc="All_MEs_are_undefined" >
    <apply>
	<eq/>
	 <apply>
	<min>
	 <bvar>i</bvar>
	  <lowlimit>
		<cn>0</cn>
	  </lowlimit>
	  <uperlimit><ci>%GroupLen({self})</ci></uperlimit>
          <apply>
               <apply>
                  <eq/>
                  <ci>{self}.State</ci>
                  <cn>-2</cn>
               </apply>
              </apply>
	</min>   
	   </apply>	 
        <cn>1</cn>
         </apply>

</DependRule>
  <DependRule Status="Unknown" Prio="1" SevLevel="None"  Dsc="All_MEs_are_undefined_or_unknown" >
    <apply>
	<and/>
    <apply>
        <eq/>
         <apply>
        <max>
         <bvar>i</bvar>
          <lowlimit>
                <cn>0</cn>
          </lowlimit>
          <uperlimit><ci>%GroupLen({self})</ci></uperlimit>
          <apply>
               <apply>
                  <eq/>
                  <ci>{self}.State</ci>
                  <cn>-1</cn>
               </apply>
              </apply>
        </max>  
           </apply>     
        <cn>1</cn>
         </apply>
	
	
    <apply>
        <eq/>
         <apply>
        <min>
         <bvar>i</bvar>
          <lowlimit>
                <cn>0</cn>
          </lowlimit>
          <uperlimit><ci>%GroupLen({self})</ci></uperlimit>
          <apply>
               <apply>
                  <lt/>
                  <ci>{self}.State</ci>
                  <cn>0</cn>
               </apply>
              </apply>
        </min>  
           </apply>     
        <cn>1</cn>
         </apply>
</apply>
</DependRule>
</StatusRulesSet>
</NGOPConfig>
