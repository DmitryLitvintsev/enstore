<?xml version='1.0'?>
<!DOCTYPE NGOPConfig SYSTEM "system.dtd">
<NGOPConfig>
<Default_File/>
<StatusRulesSet ID="DefaultMERules">
<GenericRule Status="Good" Prio="0" SevLevel="None" >
         <apply>
            <apply>
               <eq/>
